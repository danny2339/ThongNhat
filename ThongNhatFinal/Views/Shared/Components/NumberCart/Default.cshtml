@model List<ThongNhatFinal.ModelViews.CartItem>
@if (Model != null && Model.Count() > 0)
{
    <a href="cart.html" id="header-cart-btn" target="_blank"><i class="ti-bag"></i><span class="cart_quantity">@Model.Count()</span></a>
}
else{
    <a href="cart.html" id="header-cart-btn" target="_blank"><i class="ti-bag"></i><span class="cart_quantity">0</span></a>
}
